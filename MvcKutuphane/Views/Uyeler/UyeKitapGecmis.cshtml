@using MvcKutuphane.Models.Entity;
@model List<TBLHAREKET>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Üye: @ViewBag.u1</h2>
<br />
<table class="table table-danger">
    <tr>
        <th>Kitap Adı</th>
        <th>Personel</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.TBLKITAP.AD</th>
            <th>@item.TBLPERSONEL.PERSONEL</th>
            <th>@Convert.ToDateTime(item.ALISTARIH).ToString("dd/MM/yyyy")</th>
            <th>@Convert.ToDateTime(item.UYEGETIRTARIH).ToString("dd/MM/yyyy")</th>
        </tr>
    }

</table>

